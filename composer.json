{
    "name": "siewwp/laravel-service-host",
    "description": "laravel server-to-server service host with HMAC authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "WP Siew",
            "email": "siewwp.and@gmail.com"
        }
    ],
    "require": {
        "symfony/psr-http-message-bridge": "~1.0",
        "zendframework/zend-diactoros": "^1.7",
        "siewwp/php-hmac-http": "^1.1.1"
    },
    "require-dev": {
        "illuminate/auth": "~5.4",
        "illuminate/database": "~5.4",
        "illuminate/http": "~5.4",
        "illuminate/notifications": "~5.4",
        "illuminate/routing": "~5.4",
        "vlucas/phpdotenv": "~2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Siewwp\\LaravelServiceHost\\ServiceHostServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Siewwp\\LaravelServiceHost\\": "src/"
        }
    }
}
